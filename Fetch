fetch('http://IP_ADDRESS:3000/login', {
  method: 'POST',
  headers: {
    'Content-Type': 'application/json'
  },
  body: JSON.stringify({
    email: email,
    password: password
  })
})
.then(res => res.json())
.then(data => {
  if (data.success) {
    localStorage.setItem('token', data.token);
    alert('تم الدخول');
  }
});
